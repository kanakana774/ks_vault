<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.GameHistoryMapper">
        <insert id="insertGameHistory">
                INSERT INTO
                	T_GAME_HISTORY (
                                TOTAL_PLAY_NUMBERS,
                                TODAY_PLAY_NUMBERS,
                                INSERT_USER_ID,
                                INSERT_DATE,
                                UPDATE_USER_ID,
                                UPDATE_DATE,
                                DELETE_FLG,
                                UPDATE_COUNT
                        )
                VALUES(
                        #{totalPlayNumbers}, 
                        #{todayPlayNumbers},
                        #{insertUserId},
                        NOW(),
                        #{updateUserId},
                        NOW(),
                        #{deleteFlg},
                        #{updateCount}
                )
        </insert>
    
</mapper>